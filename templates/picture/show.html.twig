{% extends 'visitor.html.twig' %}
{% block title %}
	{{ parent()}}
	- Index Images
{% endblock %}
{% block stylesheets %}
	{{ parent() }}
	{{ encore_entry_link_tags('album') }}
{% endblock %}
{% block body %}
    <h1>Picture</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ picture.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ picture.title }}</td>
            </tr>
            <tr>
                <th>FileName</th>
                <td>{{ picture.fileName }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ picture.createdAt }}</td>
            </tr>
            <tr>
                <th>IsPrivate</th>
                <td>{{ picture.isPrivate ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>IsAlbumCover</th>
                <td>{{ picture.isAlbumCover ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_picture_index') }}">back to list</a>

    <a href="{{ path('app_picture_edit', {'id': picture.id}) }}">edit</a>

    {{ include('picture/_delete_form.html.twig') }}
{% endblock %}
